/*居中对齐*/
.commonHead {
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  transition: all .3s ease-out;
  text-align: center;
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
  z-index: 1000;
}
.commonHead_content {
  /* font-size: 48rpx; */
}
.commonHead_left, .commonHead_right, .commonHead_content {
  padding: 0 36rpx;
  transition: all .3s ease-out;
}
.commonHead_left {
  text-align: left;
}